<h1>Users#signup</h1>

<h1>Signup!</h1>

<%= form_for :user, url: signup_path(@user), method: :post do |f| %>

  Name: <%= f.text_field :login %>
  Password: <%= f.password_field :password %>
  Password Confirmation: <%= f.password_field :password_confirmation %>
  <%= f.submit "Submit" %>

<% end %>
